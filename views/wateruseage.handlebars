<script>
  let txtday = document.getElementById('textday');
  let txthours = document.getElementById('texthours');
  function clock() {
    let today = new Date();
    let month = today.getMonth() + 1;
    let date = today.getDate();
    let day = today.getDay();
    let charday = "";
    switch (day) {
      case 0: charday = "일요일";
        break;
      case 1: charday = "월요일";
        break;
      case 2: charday = "화요일";
        break;
      case 3: charday = "수요일";
        break;
      case 4: charday = "목요일";
        break;
      case 5: charday = "금요일";
        break;
      case 6: charday = "토요일";
        break;
    }
    let hours = today.getHours();
    let minutes = today.getMinutes();
    let daytext = month + "월 " + date + "일 " + charday;

    txtday.innerText = daytext;
  }
  
  function clockminute() {
    let today = new Date();
    let hours = today.getHours();
    let minutes = today.getMinutes();
    let daytext = hours + " : " + minutes;

    txthours.innerText = daytext;
  }
  setInterval(clock, 1000);
  setInterval(clockminute, 1000);

  var socket = io();
  socket.on('currentT1H', (msg) => {
    var txt = document.getElementById('currentT1H');
    txt.innerText = msg;
  });

  socket.on('weekendwater', (msg) => {
    var txt = document.getElementById('weekendwater');
    var todaywateruseage = document.getElementById('todaywateruseage');

    todaywateruseage.innerText = msg;
    txt.innerText = msg;
  });

  socket.on('waterpercent', (msg) => {
    var txt1 = document.getElementById('waterpercent');
    var todaywaterpercent = document.getElementById('todaywaterpercent');
    var txt2 = document.getElementById('waterpercent1');
    var txt3 = document.getElementById('Arraypercent');
    var graphpercent2 = document.getElementById("graphpercent2");
    var graphpercent3 = document.getElementById("graphpercent3");
    var graphpercent4 = document.getElementById("graphpercent4");
    var graphpercent5 = document.getElementById("graphpercent5");
    var graphpercent6 = document.getElementById("graphpercent6");
    var graphpercent7 = document.getElementById("graphpercent7");

    var showpercent1 = document.getElementById("showpercent1");
    var showpercent2 = document.getElementById("showpercent2");
    var showpercent3 = document.getElementById("showpercent3");
    var showpercent4 = document.getElementById("showpercent4");
    var showpercent5 = document.getElementById("showpercent5");
    var showpercent6 = document.getElementById("showpercent6");
    var showpercent7 = document.getElementById("showpercent7");

    todaywaterpercent.style.height = msg[0] + "%";
    graphpercent2.style.height = msg[1] + "%";
    graphpercent3.style.height = msg[2] + "%";
    graphpercent4.style.height = msg[3] + "%";
    graphpercent5.style.height = msg[4] + "%";
    graphpercent6.style.height = msg[5] + "%";
    graphpercent7.style.height = msg[6] + "%";

    showpercent1.innerText = msg[0];

    showpercent2.innerText = msg[1];
    showpercent3.innerText = msg[2];
    showpercent4.innerText = msg[3];
    showpercent5.innerText = msg[4];
    showpercent6.innerText = msg[5];
    showpercent7.innerText = msg[6];

    txt1.innerText = msg[0];
    txt2.innerText = msg[1];
    txt3.innerText = msg;
  });



</script>